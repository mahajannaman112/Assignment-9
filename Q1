#include <iostream>
#include <list>
#include <queue>
using namespace std;

class Graph {
    int V;
    list<int> *adj;

public:
    Graph(int v) {
        V = v;
        adj = new list<int>[V+1];
    }

    void addEdge(int u, int v) {
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    void BFS(int src) {
        int visited[100] = {0};
        queue<int> q;

        visited[src] = 1;
        q.push(src);

        cout << "BFS: ";

        while(!q.empty()) {
            int node = q.front();
            q.pop();
            cout << node << " ";

            for(int neigh : adj[node]) {
                if(!visited[neigh]) {
                    visited[neigh] = 1;
                    q.push(neigh);
                }
            }
        }
    }
};

int main() {
    int n, m;
    cin >> n >> m;

    Graph g(n);

    for(int i=0;i<m;i++){
        int u, v;
        cin >> u >> v;
        g.addEdge(u, v);
    }

    int src;
    cin >> src;

    g.BFS(src);
}
